<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vedantgolash.tourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tour"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".malls"
            android:label="Shopping Malls"></activity>
        <activity android:name=".hotel"
            android:label="Hotels"/>
        <activity android:name=".food"
            android:label="Food Places"/>
        <activity android:name=".Historical"
            android:label="Historical Places"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>